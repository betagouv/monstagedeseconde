= form_with model: model, url: url, method: method, data: { turbo: false } do |form|
  = render "dashboard/stepper/steps",
            current_step_number: 2,
            total_steps: 4,
            current_step_title: "Le coordinateur",
            next_step_title: "Les structures accueillantes"

  = render "layouts/form_errors",
           resource: @multi_coordinator,
           resource_name: :multi_coordinator

  = render 'dashboard/multi/multi_coordinators/form_fields',
            resource: @multi_coordinator,
            form: form,
            submit_button: submit_button,
            show_resume: show_resume

