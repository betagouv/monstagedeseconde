<!DOCTYPE html>
<html lang="fr" >
  <head>
    <title>Erreur</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= action_cable_meta_tag %>
    <%= favicon_link_tag vite_asset_path('images/favicon.png') %>
    <%= vite_javascript_tag "application.js", 'data-turbo-track': "reload", defer: true %>
  </head>

  <body class=''>
    <div class="content pt-0" data-controller='rg2a'>
      <header role="banner" class="custom-navbar">
        <div class="col px-0">
          <%= render "layouts/navbar" %>
        </div>
      </header>
      <main>
        <%= yield %>
      </main>
    </div>
  </body>
</html>
